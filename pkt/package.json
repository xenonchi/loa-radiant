{
    "name": "pkt-processor",
    "version": "0.0.0",
    "private": true,
    "type": "commonjs",
    "exports": {
        "./*": {
            "require": "./dist/*.js",
            "import": "./dist/*.mjs",
            "types": "./dist/*.d.ts"
        }
    },
    "scripts": {
        "format": "prettier --write \"**/*.{js,json,ts}\"",
        "build-dist": "tsc --noEmit && tsup api.ts --clean --no-config --sourcemap",
        "pkt": "tsc --noEmit && tsup api.ts --clean --no-config --sourcemap && node dist/api.js"
    },
    "dependencies": {
        "@types/express": "^4.17.21",
        "@types/ws": "^8.5.10",
        "axios": "^1.3.6",
        "cap": "^0.2.1",
        "is-admin": "^3.0.0",
        "lodash": "^4.17.21",
        "lz4-napi": "^2.2.0",
        "meter-core": "github:lost-ark-dev/meter-core",
        "oodle": "github:Mathicha/oodle",
        "raw-socket-sniffer": "github:Herysia/raw-socket-sniffer",
        "snappyjs": "^0.7.0",
        "tiny-typed-emitter": "^2.1.0",
        "ts-node": "^10.9.2",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@tsconfig/node18": "^2.0.0",
        "@tsconfig/strictest": "^2.0.1",
        "@types/lodash": "^4.14.202",
        "@types/node": "^18.11.11",
        "prettier": "^2.8.1",
        "tsup": "^6.7.0",
        "typescript": "^5.3.3"
    }
}
